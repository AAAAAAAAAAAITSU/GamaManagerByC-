<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GameManagerApp.ViewModels"
                    xmlns:view="clr-namespace:GameManagerApp.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>
        
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Customers}">
        <view:Customers/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Products}">
        <view:Products/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OrdersVM}">
        <view:Orders/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Transactinos}">
        <view:Transactions/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:Shipments}">
        <view:Shipments/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <view:Setting/>
    </DataTemplate>
</ResourceDictionary>